    "hono": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:",
    "@ouzx-me/eslint-config": "workspace:*",
    "@ouzx-me/typescript-config": "workspace:*",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  }
}
