{
  "name": "@ouzx-me/web",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
